package package1;

public class class1 {
    public int x = 5;
    int y = 10;

    void m1() {
        System.out.println(x);
        System.out.println(y);
    }

    public static void main(String[] args) {
        System.out.println("This is main method of class-1");
    }
}

package package1;

public class Class2 {
    public static void main(String[] args) {
        Class1 c1 = new Class1();
        System.out.println(c1.x);
        System.out.println(c1.y);
    }
}
