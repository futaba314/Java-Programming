public class SubtractionQuiz {
    public static void main(String[] args) {
        for (int i = 0; i < 10; i++) {
            int num1 = (int)(Math.random() * 100); 
            int num2 = (int)(Math.random() * 100);
            int result = num1 - num2;
            System.out.println(num1 + " - " + num2 + " =?");
        }
    }
}
