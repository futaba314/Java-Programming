public class Test {
    public static void main(String[] args) {
        int sum = 0;
        for (int i = 0; i < 10; i++) {
            sum += i;
        }
        int j = 0;
        while (j < 10) {
            if (i < j) {
                System.out.println(i);
            }
            j++;
        }
        j = 0;
        do {
            j++;
        } while (j < 10);
        System.out.println(j);
    }
}
