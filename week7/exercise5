public class Lab4Task5 {
    public static void nPrintln(String message, int n) {
        for (int i = 0; i < n; i++) {
            System.out.println(message);
        }
    }
    public static void main(String[] args) {
        String message = "Hello, World!";
        int n = 3;
        nPrintln(message, n);
    }
}
